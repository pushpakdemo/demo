pipeline{
     agent any
	          stages{
			       stage('one') {
			          steps {
					           echo 'hi, this is pushpak'
							 }
					}
			   stage('two') {
			          steps { 
					           input('do you wnat to proceed?')
						   }
					}
               stage('three') {
                       when {
                                not {
                                       branch "master"
                                    }
                    }									   
                   steps { 
				          echo "hello world"
                       }
                    }
			    
		}	
	}		
			
